document.addEventListener("DOMContentLoaded",()=>{let t=document.querySelector(".js-header-button-wrap"),n=document.querySelector(".js-header-menu"),o=document.body,s=document.querySelector(".js-sp-button");var e=document.querySelector(".js-footer");t&&n&&t.addEventListener("click",()=>{var e=t.classList.contains("active");t.classList.toggle("active",!e),n.classList.toggle("open",!e),o.classList.toggle("no-scroll",!e),s&&s.classList.toggle("not-visible",!0==!e)}),s&&e&&new IntersectionObserver(([e])=>{o.classList.contains("open")||(e.isIntersecting?s.classList.add("is-hidden"):s.classList.remove("is-hidden"))},{root:null,threshold:0}).observe(e)}),document.addEventListener("DOMContentLoaded",function(){document.querySelectorAll(".faq__details").forEach(function(e){e.querySelector(".faq__summary").addEventListener("click",function(){e.classList.toggle("open")})})}),document.addEventListener("DOMContentLoaded",function(){document.querySelectorAll(".price__details").forEach(function(e){e.querySelector(".price__summary").addEventListener("click",function(){e.classList.toggle("open")})})}),document.addEventListener("wpcf7mailsent",function(e){window.location.href=cf7Redirect.thanksUrl},!1);
